{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "*.csproj"
                    ],
                    "src": "./"
                }
            ],
            "dest": "../DocFx/reference",
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "filter": "../DocFx/filterConfig.yml"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "reference/**.yml",
                    "reference/index.md"
                ],
                "src": "../DocFx"
            },
            {
                "files": [
                    "*.md",
                    "toc.yml"
                ],
                "src": "../DocFx"
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ],
                "src": "../DocFx"
            }
        ],
        "dest": "../docs",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false,
        "globalMetadata": {
            "_appTitle": "Documentation",
            "_enableSearch": true,
            "_disableContribution": true
        }
    }
}