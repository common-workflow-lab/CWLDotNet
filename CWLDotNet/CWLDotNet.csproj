<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>CWLDotNet</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <EnablePreviewFeatures>True</EnablePreviewFeatures>
    <PackageId>CWLDotNet</PackageId>
    <Title>CWLDotNet</Title>
    <Version>0.1.0</Version>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Authors>Adrian Zimmer</Authors>
    <PackageTags>cwl;cwldotnet;commonwl;common workflow language;dotnet;models;parsing;serialization;deserialization;workflow;workflows</PackageTags>
    <Description>
      This project contains .Net objects and utilities auto-generated by https://github.com/common-workflow-language/schema_salad for parsing documents corresponding to the https://w3id.org/cwl/cwl schema.
    </Description>
    <PackageProjectUrl>https://common-workflow-lab.github.io/CWLDotNet/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/common-workflow-lab/CWLDotNet.git</RepositoryUrl>
    <BuildDocFx Condition=" '$(Configuration)'=='Debug' ">false</BuildDocFx>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="docfx.console" Version="2.59.2">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.CodeStyle" Version="4.2.0">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Roslynator.Formatting.Analyzers" Version="4.1.1">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="YamlDotNet" Version="11.2.1" />
    <PackageReference Include="OneOf" Version="3.0.216" />
      <PackageReference Include="OneOf.Extended" Version="3.0.216" />
  </ItemGroup>
</Project>
