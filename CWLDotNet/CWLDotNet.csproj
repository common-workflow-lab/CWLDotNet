<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>CWLDotNet</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <EnablePreviewFeatures>True</EnablePreviewFeatures>
    <PackageId>CWLDotNet</PackageId>
    <Version>0.1.0</Version>
    <Authors>Adrian Zimmer</Authors>
    <BuildDocFx Condition=" '$(Configuration)'=='Debug' ">false</BuildDocFx>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="docfx.console" Version="2.59.2">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.CodeStyle" Version="4.2.0">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Roslynator.Formatting.Analyzers" Version="4.1.1">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="YamlDotNet" Version="11.2.1" />
    <PackageReference Include="OneOf" Version="3.0.216" />
      <PackageReference Include="OneOf.Extended" Version="3.0.216" />
  </ItemGroup>
</Project>
